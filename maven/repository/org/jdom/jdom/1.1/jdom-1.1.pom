      <artifactId>jaxen-core</artifactId>
      <version>1.0-FCS</version>  <!-- patched 1.0-FCS -->
      <optional>true</optional>
    </dependency>
    <dependency>
      <groupId>org.jdom</groupId>
      <artifactId>jaxen-jdom</artifactId>
      <version>1.0-FCS</version>  <!-- patched 1.0-FCS -->
      <optional>true</optional>
    </dependency>
    <dependency>
