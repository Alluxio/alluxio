{
  "name": "@alluxio/alluxio-ui-master",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@alluxio/common-ui": "~1.0.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.12",
    "@fortawesome/free-brands-svg-icons": "^5.6.3",
    "@fortawesome/free-regular-svg-icons": "^5.6.3",
    "@fortawesome/free-solid-svg-icons": "^5.6.3",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "axios": "~0.18.0",
    "babel-polyfill": "~6.26.0",
    "bootstrap": "~4.1.3",
    "connected-react-router": "~5.0.1",
    "react": "~16.6.3",
    "react-dom": "~16.6.3",
    "react-helmet": "~5.2.0",
    "react-redux": "~5.1.1",
    "react-router-dom": "~4.3.1",
    "react-scripts-ts": "~3.1.0",
    "reactstrap": "~6.5.0",
    "redux": "~4.0.1",
    "redux-saga": "~0.16.2",
    "source-sans-pro": "^2.40.0",
    "typesafe-actions": "~2.0.4"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "PORT=3001 react-app-rewired start --scripts-version react-scripts-ts --watch --recursive",
    "build-js": "react-app-rewired build --scripts-version react-scripts-ts",
    "start": "npm-run-all --parallel watch-css start-js",
    "build": "npm-run-all --parallel build-css build-js",
    "test": "react-app-rewired test --scripts-version react-scripts-ts --env=jsdom --watch",
    "test-ci": "CI=true react-app-rewired test --scripts-version react-scripts-ts --env=jsdom",
    "coverage": "react-app-rewired test --scripts-version react-scripts-ts --env=jsdom --coverage --collectCoverageFrom=src/**/*tsx --collectCoverageFrom=!src/*tsx --watch",
    "coverage-ci": "CI=true react-app-rewired test --scripts-version react-scripts-ts --env=jsdom --coverage --collectCoverageFrom=src/**/*tsx --collectCoverageFrom=!src/*tsx",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/bootstrap": "~4.1.3",
    "@types/enzyme": "~3.1.15",
    "@types/enzyme-adapter-react-16": "~1.0.3",
    "@types/jest": "~23.3.10",
    "@types/node": "~10.12.10",
    "@types/react": "~16.7.9",
    "@types/react-dom": "~16.0.11",
    "@types/react-helmet": "~5.0.7",
    "@types/react-redux": "~6.0.10",
    "@types/react-router-dom": "~4.3.1",
    "@types/reactstrap": "~6.4.3",
    "enzyme": "~3.7.0",
    "enzyme-adapter-react-16": "~1.7.0",
    "enzyme-to-json": "~3.3.4",
    "node-sass-chokidar": "~1.3.4",
    "npm-run-all": "~4.1.5",
    "react-app-rewired": "~1.6.2",
    "react-test-renderer": "~16.6.3",
    "redux-devtools-extension": "~2.13.7",
    "typescript": "~3.2.1"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "engines": {
    "node": "10.11.0",
    "npm": "6.4.1"
  }
}
